{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "sourceMap": true,
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "lib": ["es6", "dom"],
    "target": "es6",
    "strict": true,
    "noImplicitAny": true
  },
  "exclude": [
    "./node_modules"
  ],
  "include": [
    "./src/server"
  ]
}